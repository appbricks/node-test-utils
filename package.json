{
  "name": "@appbricks/test-utils",
  "version": "0.0.3",
  "description": "Node Typescript Module Template",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --quiet --fix",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appbricks/node-test-utils.git"
  },
  "keywords": [
    "AppBricks"
  ],
  "author": "Mevan Samaratunga",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/appbricks/node-test-utils/issues"
  },
  "homepage": "https://github.com/appbricks/node-test-utils",
  "dependencies": {
    "@appbricks/utils": "0.0.6",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
